{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "xv6build",
            "type": "shell",
            "isBackground": true,
            "command": "docker exec oslab make qemu-gdb",
            "problemMatcher": [
                {
                    "owner": "xv6",
                    "pattern": [
                        {
                            "regexp": ".",
                            "file": 1,
                            "location": 2,
                            "message": 3
                        }
                    ],
                    "background": {
                        "beginsPattern": ".*", 
                        "endsPattern": "tcp::26000"
                    }
                }
            ]
        }
    ]
}
